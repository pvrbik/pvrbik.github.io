<Group labelreference="L93" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">hack:=proc(n) local x0,x1,y0,y1,ans,now:    (x0,y0):=findColl(n):    (x1,y1):=findColl(n):    ans:=gcd(abs(x0-y0),abs(x1-y1)):        while (true) do                (x0,y0):=findColl(p,q):        now:=gcd(ans,abs(x0,y0)):        if (now=ans) then return ans:        else ans:=now: end if:    end do:end proc:</Text-field>
</Input>
